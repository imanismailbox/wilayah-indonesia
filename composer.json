{
  "name": "badak/indonesia",
  "description": "Package Laravel yang berisi data Provinsi, Kabupaten/Kota, Kecamatan, dan Keluarahan/Desa di seluruh Indonesia. clone from badak/indonesia",
  "keywords": [
    "laravel",
    "laravolt",
    "indonesia",
    "provinsi",
    "Kabupaten",
    "kota",
    "kecamatan",
    "kelurahan",
    "desa"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Iman Ismail",
      "email": "imanismailbox@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Badak\\Indonesia\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Badak\\Indonesia\\Test\\": "tests/"
    }
  },
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "php-coveralls/php-coveralls": "^2.1",
    "orchestra/testbench": "^6.0|^7.0|^8.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Badak\\Indonesia\\ServiceProvider"
      ],
      "aliases": {
        "Indonesia": "Badak\\Indonesia\\Facade"
      }
    }
  }
}
